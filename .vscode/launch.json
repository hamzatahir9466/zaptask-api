{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Jest Test",
      "program": "${workspaceFolder}/node-service/node_modules/jest/bin/jest.js",
      "args": [
        "test/task.test.js"
      ],
      "runtimeArgs": [
        "--experimental-vm-modules"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true
    }
  ]
}
